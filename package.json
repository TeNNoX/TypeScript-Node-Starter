{
    "name": "express-typescript-starter",
    "version": "0.1.0",
    "description": "A starting point for Node.js express apps with TypeScript",
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/TypeScript-Node-Starter"
    },
    "author": "Bowden Kelly",
    "license": "MIT",
    "scripts": {
        "build": "yarn run build-ts",
        "build-ts": "tsc",
        "copy-static-assets": "ts-node copyStaticAssets.ts",
        "debug": "yarn run build && yarn run watch-debug",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "serve": "node dist/server.js",
        "serve-debug": "nodemon --inspect dist/server.js",
        "start": "yarn run serve",
        "test": "jest --forceExit --coverage --verbose",
        "watch": "concurrently -k -p \"[{name}]\" -n \"SasTypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run watch-node\"",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run serve-debug\"",
        "watch-node": "nodemon dist/server.js",
        "watch-test": "yarn run test -- --watchAll",
        "watch-ts": "tsc -w"
    },
    "dependencies": {
        "async": "^3.1.0",
        "axios": "^0.21.0",
        "dotenv": "^8.2.0",
        "errorhandler": "^1.5.1",
        "lodash": "^4.17.19",
        "nedb": "^1.8.0",
        "pino": "^6.7.0",
        "telebot": "^1.4.1",
        "xml-query": "^1.5.0",
        "xml-reader": "^2.4.3"
    },
    "devDependencies": {
        "@types/async": "^3.0.2",
        "@types/chai": "^4.2.3",
        "@types/concurrently": "^4.1.0",
        "@types/errorhandler": "^0.0.32",
        "@types/eslint": "^6.1.1",
        "@types/jest": "^24.0.23",
        "@types/jquery": "^3.3.31",
        "@types/lodash": "^4.14.141",
        "@types/nedb": "^1.8.11",
        "@types/node": "^12.7.8",
        "@types/pino": "^6.3.3",
        "@types/request": "^2.48.3",
        "@types/request-promise": "^4.1.44",
        "@types/shelljs": "^0.8.5",
        "@types/supertest": "^2.0.8",
        "@types/telebot": "^1.2.1",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^4.7.0",
        "@typescript-eslint/parser": "^4.7.0",
        "chai": "^4.2.0",
        "concurrently": "^5.3.0",
        "eslint": "7",
        "eslint-config-standard-with-typescript": "^19.0.1",
        "eslint-plugin-import": "2",
        "eslint-plugin-node": "11",
        "eslint-plugin-promise": "4",
        "eslint-plugin-standard": "4",
        "nodemon": "^1.19.2",
        "pino-pretty": "^4.3.0",
        "shelljs": "^0.8.3",
        "supertest": "^4.0.2",
        "ts-jest": "^24.1.0",
        "ts-node": "^8.4.1",
        "typescript": "^3.6.3"
    }
}
